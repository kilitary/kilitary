body
    font-family "Lucida Console"
    font-size 12px
    padding-left 0px
    height 100%
    width 98%

.content-container
    box-shadow 0 0px 0px #2abaff, 0 0px 32px 0 #ffefa2
    padding-top 5px
    padding-bottom 5px
    padding-left 15px
    background-color #fefff5

#flagleft
    display inline-block
    position absolute
    width 84px
    height 100px
    left 10px
    top -16px
    float left
    background-image url(/images/11.png)
    background-position-x 82px
    background-size contain
    image-rendering pixelated

.ip-info
    font-family "consolas" !important
    font-size 12px !important

#flagright
    display flex
    top -16px
    width 83px
    height 140px
    position absolute
    left 96%
    float left
    background-image url(/images/usflagged.jpg)
    background-position-x 2px
    background-size contain
    border-top 11px dashed deepskyblue
    object-fit cover
    image-rendering pixelated

#app
    border-left 9px dashed cyan
    padding-left 9px
    animation falling-red 27.3s infinite

button, input, textarea
    box-shadow 0 0px 23px 0 #ffd023, 0 0px 0px 0 #ff0081
    font-family lucida console
    background-color white
    cursor pointer
    border 2px dashed darkorange

    &:hover
        box-shadow 0px 1px 15px #a1ff25, 0 0px 0px 0 #ff0081
        background-color #fff9be !important
        border 2px dashed red !important

.content-keys
    font-family "lucida console"
    color seagreen
    padding-left 15px
    font-variant small-caps
    font-size 10px

.crysa-class
    height 104px
    opacity 0.9
    position absolute
    top -24px
    left 1150px
    image-rendering pixelated

.commit-button
    background-image url('/images/_n.jpg')
    background-repeat no-repeat
    background-position-x right
    background-size cover
    color white
    transition background 11s
    background-blend-mode difference
    width 200px
    height 30px

.url-input
    width 300px
    margin-top 3px
    margin-bottom 5px

blockquote
    border-left 3px solid blueviolet
    margin-left 0px
    padding-left 3px

ul
    list-style-type circle

.section-h3
    border-bottom 1px solid #ebf8ff
    padding-bottom 11px
    display border-block
    font-size 16px
    width 20%
    font-variant small-caps
    clear both

.container
    width 100%
    height 100%
    display block

.list-item-container
    border-bottom 1px dashed lightblue
    padding 4px
    display box
    min-height 10px

.marginleft
    margin 12px

.smallcaps
    font-variant small-caps
    font-family consolas
    font-size 17px

.super-small
    font-size 4px
    font-family "Lucida Console"
    font-variant small-caps

.topheadmargin
    margin-top 10px
    padding-top 12px

.interestlink
    font-size 12px
    padding 3px
    font-family lucida console
    box-shadow 0 2px 8px #45ff08, 0 1px 1px 0 #34daff

    &:hover
        box-shadow 1px 1px 1px red, 0 0px 1px 0 #34daff


.select-doc-future
    padding-left 15px
    margin-left 0px

    & li
        padding-top 5px

.tib-war-header
    font-variant small-caps
    font-size 16px
    display inline

.comment
    display flow-root
    margin-bottom 8px
    padding-bottom 5px
    box-shadow 18px 3px 0px #24b8ff, 0 0px 5px 0 #b0ffd7

.new-comment
    padding 10px

.text-comment
    width 25%
    height 100px
    clear both
    color black
    font-family lucida console
    opacity 1

.comment-img
    position relative
    top 4px
    width 12px
    height 12px

.comment-date
    font-size 12px
    font-family consolas

.comment-content
    padding-left 18px
    font-size 12px
    font-family consolas

.page-content
    padding-top 0px
    margin-top 9px

.page-header
    border-top 9px dotted lightgray
    padding-bottom 0px
    padding-top 11px
    margin-top 0px

.construct-logo
    padding-top 10px

    &:hover
        width 240px
        height 240px
        border 5px solid mediumspringgreen

.page-footer
    border-top 9px dotted lightgray
    padding-bottom 0px
    padding-top 0px
    margin-top 0px

.log-record
    font-family consolas
    font-size 8px
    border-bottom 4px dashed orange
    margin-bottom 10px

.car
    position absolute
    top 25%
    right 3%
    opacity 0.15

.log-code
    font-family consolas
    display inline-flex
    width 22px

.log-ip
    font-family consolas
    display block
    color lightseagreen

.log-info
    font-family consolas
    display table
    width 100%
    word-break keep-all
    white-space pre-wrap

.log-date
    font-family consolas
    display inline-flex
    width 134px
    word-break break-all
    color lightslategray

.log-method
    font-family consolas
    display inline-flex
    width 24px
    color #e83e8c

.log-ua
    font-family consolas
    display table-footer-group
    width 100px
    color lightskyblue

.log-url
    font-family consolas
    display flex
    width 100px
    color yellowgreen

.marginbottom
    margin-bottom 5px

.list-key
    font-family "consolas"
    font-size 11px
    width 10%
    display inline
    text-align right
    border-right 2px dashed greenyellow
    padding-right 8px
    margin-right 1px
    float left
    box-shadow 14px 24px 6px #f5f5f5, 1px 0px 0px 0px #ff0081

    &:hover
        background-color lawngreen
        color red
        cursor pointer

.list-item
    font-family "consolas"
    font-size 11px
    display inline
    padding-left 2px
    margin 10px
    width 90%
    box-shadow 0px 1px 6px #a1ff25, 0 0px 0px 0 #ff0081

    &:hover
        background-color deeppink
        color white
        cursor pointer

.links
    padding-top 10px

.page-country
    padding-top 5px
    padding-bottom 5px

.ignited
    color #20c997
    margin-bottom 0px
    padding-bottom 0px

.wide-button
    padding 2px
    margin 0px
    margin-top 4px !important
    margin-left 10px


a, a:visited
    text-decoration none
    color #117a8b

a:hover
    color red

.blinking-red
    animation blinkingRed 0.1s infinite

    @keyframes blinkingRed {
        0% {
            color red
        }
        50% {
            color transparent
        }
        100% {
            color red
        }
    }

.blinking-green
    animation blinkingGreen 1.3s infinite

.shortener
    margin 10px

    input
        margin-left 10px

    button
        padding 3px
        margin-top 4px

@keyframes blinkingGreen {
    0% {
        color green
    }
    50% {
        color #955ae7
    }
    100% {
        color greenyellow
    }
}

.blinking-orange
    animation blinkingOrange 1.8s infinite

@keyframes blinkingOrange {
    0% {
        color yellow
    }
    50% {
        color orange
    }
    100% {
        color red
    }
}

@keyframes falling-red {
    20% {
        border-left 9px dashed cyan
    }
    60% {
        border-left 9px dashed #a1ff25
    }

    80% {
        border-left 9px dashed red
    }
}
