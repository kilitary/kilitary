body
    font-family: "Lucida Console"
    font-size: 12px
    border-left: 2px dashed orange
    padding-left: 10px
    height: 100%
    width: 95%

.container
    width: 100%
    height: 100%
    border-bottom: 1px dashed red

.list-item-container
    border-bottom: 1px dashed lightblue
    padding: 4px
    padding-left: 4px

.list-key
    font-family: "consolas"
    font-size: 11px
    width: 100px
    display: inline-block
    text-align: right

.list-item
    font-family: "consolas"
    font-size: 11px

.links
    padding-top: 10px

a
a:visited
    text-decoration none

a:hover
    border-bottom: 3px dotted red

.blinking-red
    animation: blinkingRed 0.8s infinite;

    @keyframes blinkingRed {
        0% {
            color: red;
        }
        50% {
            color: transparent;
        }
        100% {
            color: red;
        }
    }

.blinking-green
    animation: blinkingGreen 1.8s infinite;

@keyframes blinkingGreen {
    0% {
        color: green;
    }
    50% {
        color: transparent;
    }
    100% {
        color: greenyellow;
    }
}

.blinking-orange
    animation: blinkingOrange 1.8s infinite;

@keyframes blinkingOrange {
    0% {
        color: green;
    }
    50% {
        color: orange;
    }
    100% {
        color: red;
    }
}
