{
    "name": "kilitary/laragen",
    "type": "project",
    "description": "The kilitary Framework.",
    "keywords": [
        "fss",
        "v2k"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5",
        "amnuts/opcache-gui": "^3.0",
        "clue/socks-react": "^1.2",
        "fideloper/proxy": "^4.2",
        "filicious/core": "^0.9.1",
        "fruitcake/laravel-cors": "^2.0",
        "geoip2/geoip2": "~2.0",
        "google/cloud-language": "^0.24.4",
        "google/cloud-logging": "^1.21",
        "google/cloud-storage": "^1.23",
        "guzzlehttp/guzzle": "6.5.x-dev",
        "intervention/image": "^2.5",
        "ipinfo/ipinfo": "*",
        "kilitary/php-web-article-extractor": "dev-master",
        "laravel/framework": "^7",
        "laravel/telescope": "^3.5",
        "laravel/tinker": "^2.0",
        "league/commonmark": "^1.5",
        "maxmind-db/reader": "~1.0",
        "phpmailer/phpmailer": "^6.1",
        "predis/predis": "^1.1",
        "react/event-loop": "^1.1",
        "react/http": "^1.1",
        "react/mysql": "^0.5.4",
        "spatie/laravel-responsecache": "^6.6"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "barryvdh/laravel-ide-helper": "^2.8",
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
